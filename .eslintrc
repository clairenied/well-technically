{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
      "document": true,
      "window": true,
      "process": true
    },
    "rules": {
      "import/extensions": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/html-has-lang": 0,
      "linebreak-style": 0,
      "react/jsx-filename-extension": 0,
      "react/prop-types": [2, { "ignore": ["children"] }],
      "react/react-in-jsx-scope": 0
    }
}
